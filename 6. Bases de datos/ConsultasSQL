USE movies_db;
select * from movies_db.movies;
select act.first_name,act.last_name, act.rating from movies_db.actors act;
select title as titutlo from series;
select title,rating,awards from movies where rating >= 7.5;
select title,rating  from movies order by rating;
select id,title,rating from movies order by id limit 3;
select title,rating from movies order by rating desc limit 5;
select first_name,last_name from actors order by id limit 10;
select title,rating from movies where title like "%Toy Story%";
select first_name,last_name from actors where first_name like "Sam%";
select title,release_date from movies where release_date >= "2004-01-01" and release_date <= "2008-12-31";
select title,release_date from movies where release_date  between "2004-01-01" and "2008-12-31";
select title,rating,awards from movies where rating > 3 and awards >= 1 and release_date  between "1998-01-01" and "2009-12-31" order by rating;